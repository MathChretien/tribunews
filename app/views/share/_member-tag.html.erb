 <div class="member-container">
  <div class="member-status">
    <!-- here show be a red or green dot for member status -->
  </div>
  <figure class="avatar-dashboard">
    <%= image_tag(current_user.photo, alt: 'avatar', height: 40, width: 40) %>
  </figure>

  <div class="member-info">
    <%= current_user.first_name %>
    <%= current_user.last_name %>
  </div>
</div>
